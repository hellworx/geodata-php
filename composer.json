{
    "name": "hellworx/hwx-geodata-php",
    "description": "Hellworx Geodata PHP Library",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "HWX\\Geodata\\": "src/HWX/Geodata"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HWX\\Geodata\\Tests\\": "test/HWX/Geodata/Tests"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "hellworx/hwx-base-php": "dev-main"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/hellworx/hwx-base-php"
        }
    ]
}
